{
  "kind": "collectionType",
  "collectionName": "faq_default_list_pages",
  "info": {
    "singularName": "faq-default-list-page",
    "pluralName": "faq-default-list-pages",
    "displayName": "FAQ Default - List Page"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "PageName": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "maxLength": 255,
      "unique": true
    },
    "ListPageType": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "enum": [
        "city",
        "area",
        "spot",
        "subway",
        "region",
        "poi",
        "cruise"
      ]
    },
    "IsDisplay": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "FAQContents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::faq-content.faq-content"
    }
  }
}
