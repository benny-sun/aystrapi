{
  "kind": "collectionType",
  "collectionName": "faq_customizeds",
  "info": {
    "singularName": "faq-customized",
    "pluralName": "faq-customizeds",
    "displayName": "FAQ Customized"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "pageName": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "maxLength": 255,
      "required": true,
      "unique": true
    },
    "listPageType": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true,
      "enum": [
        "city",
        "area",
        "spot",
        "subway",
        "region",
        "poi",
        "cruise"
      ]
    },
    "listId": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true
    },
    "faqContents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::faq-content.faq-content"
    }
  }
}
